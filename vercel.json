{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "demo/dist/**",
      "use": "@vercel/static-build"
    }
  ],
  "outputDirectory": "demo/",
  "buildCommand": "npm run build",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}
